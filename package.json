{
  "name": "cv-bench-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "concurrently \"turbo run dev\" \"mongoku start\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.276.0",
    "concurrently": "^7.6.0"
  },
  "packageManager": "npm@8.19.2"
}

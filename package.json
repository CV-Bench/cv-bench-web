{
  "name": "cv-bench-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "concurrently \"mongoku start\" \"dotenv -- turbo dev\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80,
    "trailingComma": "none",
    "importOrder": [
      "@/.*",
      "types",
      "^([.][.])/?.*$",
      "^[.]/.*$"
    ],
    "importOrderSeparation": true
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.276.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "mongoku": "^1.3.0",
    "swr": "^2.0.3"
  },
  "packageManager": "npm@8.19.2"
}
